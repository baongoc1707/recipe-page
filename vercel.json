{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/ASSETS/(.*)",
            "dest": "ASSETS/$1"
        },
        {
            "src": "/CSS/(.*)",
            "dest": "CSS/$1"
        },
        {
            "src": "/JS/(.*)",
            "dest": "JS/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}