{
    "version": 2,
    "builds": [
        {
            "src": "HTML/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/HTML/index.html"
        },
        {
            "src": "/CSS/(.*)",
            "dest": "CSS/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/HTML/index.html"
        }
    ]
}